<p id="notice"><%= notice %></p>

<h1>Atendimentos</h1>

<table>
  <thead>
    <tr>
      <th>Etapa</th>
      <th>Data</th>
      <th>Pessoa</th>
      <th>Endereco</th>
      <th>Operadora</th>
      <th>Portfolio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atendimentos.each do |atendimento| %>
      <tr>
        <td><%= atendimento.etapa %></td>
        <td><%= atendimento.data %></td>
        <td><%= atendimento.pessoa_id %></td>
        <td><%= atendimento.endereco_id %></td>
        <td><%= atendimento.operadora %></td>
        <td><%= atendimento.portfolio %></td>
        <td><%= link_to 'Show', atendimento %></td>
        <td><%= link_to 'Edit', edit_atendimento_path(atendimento) %></td>
        <td><%= link_to 'Destroy', atendimento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atendimento', new_atendimento_path %>
